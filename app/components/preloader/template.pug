#preloader
  .preloader__stiker(style='transform: rotate(-8deg);')
    .preloader__stiker__warp.preloader__stiker--to-right
      .preloader__stiker__front
        .preloader__stiker__content
          | 99
          span %
        .preloader__stiker__back
  .preloader__stiker(style='transform: rotate(2deg);')
    .preloader__stiker__warp.preloader__stiker--to-left
      .preloader__stiker__front
        .preloader__stiker__content
          | 66
          span %
        .preloader__stiker__back
  .preloader__stiker(style='transform: rotate(-5deg);')
    .preloader__stiker__warp.preloader__stiker--to-right
      .preloader__stiker__front
        .preloader__stiker__content
          | 33
          span %
        .preloader__stiker__back
  .preloader__stiker(style='transform: rotate(5deg);')
    .preloader__stiker__warp.preloader__stiker--to-left
      .preloader__stiker__front
        .preloader__stiker__content
          | 0
          span
            | %
        .preloader__stiker__back
script.
  runPreloader(false);
// END: preloader
